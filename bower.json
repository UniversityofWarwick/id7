{
  "name": "warwick-id7",
  "description": "CSS, JavaScript and sample HTML in order to use the corporate identity on your site",
  "version": "1.3.1",
  "homepage": "http://id7.warwick.ac.uk",
  "main": [
    "less/id7.less",
    "dist/css/id7.css",
    "dist/css/id7-default-theme.css",
    "dist/js/id7-bundle.js",
    "dist/fonts/fontawesome-webfont.eot",
    "dist/fonts/fontawesome-webfont.svg",
    "dist/fonts/fontawesome-webfont.ttf",
    "dist/fonts/fontawesome-webfont.woff",
    "dist/fonts/fontawesome-webfont.woff2",
    "dist/images/masthead-logo-bleed-sm.svg",
    "dist/images/masthead-logo-bleed-sm.png",
    "dist/images/masthead-logo-bleed-xs.svg",
    "dist/images/masthead-logo-bleed-xs.png",
    "dist/templates/base.html"
