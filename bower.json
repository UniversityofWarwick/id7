  "version": "0.0.1",
  "homepage": "http://id7.warwick.ac.uk",
  "main": [
    "less/id7.less",
    "dist/css/id7.css",
    "dist/js/id7-standalone.js",
    "dist/fonts/fontawesome-webfont.eot",
    "dist/fonts/fontawesome-webfont.eot.svg",
